{
    "name": "jimlind/tivo-php",
    "description": "Communicate with an S3 TiVo via Guzzle",
    "keywords": ["tivo", "guzzle"],
    "homepage": "https://github.com/jimlind/tivo-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Lind",
            "homepage": "http://twitter.com/JimLindForPope"
        }
    ],

    "autoload": {
        "psr-4": {
            "JimLind\\TiVo\\": "src/JimLind/TiVo/"
        }
    },

    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.2",
        "psr/log": "^1.0",
        "symfony/process": "^3.2"
    },

    "require-dev": {
        "apix/log": "^1.2",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.8",
        "escapestudios/symfony2-coding-standard": "^2.9",
        "satooshi/php-coveralls": "^1.0"
    }
}
