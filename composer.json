{
    "name": "jimlind/tivo-php",
    "description": "Communicate with an S3 TiVo via Guzzle",
    "keywords": ["tivo", "guzzle"],
    "homepage": "https://github.com/jimlind/tivo-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Lind",
            "homepage": "http://twitter.com/JimLindForPope"
        }
    ],

    "autoload": {
        "psr-4": {
            "JimLind\\TiVo\\": "src/JimLind/TiVo/"
        }
    },

    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^6.1",
        "psr/log": "^1.0",
        "symfony/process": "^2.7"
    },

    "require-dev": {
        "apix/log": "^1.1",
    	"phpunit/phpunit": "^4.8",
    	"squizlabs/php_codesniffer": "^2.3",
        "escapestudios/symfony2-coding-standard": "^2.7",
        "satooshi/php-coveralls": "^0.6"
    }
}
